<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StatusView">
        <attr name="hideCheckbox" format="boolean"/>
        <attr name="buttonText" format="string"/>
    </declare-styleable>
    <declare-styleable name="HeaderView">
        <attr name="hideSeparator" format="boolean"/>
        <attr name="headerText" format="string"/>
    </declare-styleable>
    <declare-styleable name="LegendEntry">
        <attr name="formColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="SegmentedControl">
        <attr name="leftTitle" format="string"/>
        <attr name="centerTitle" format="string"/>
        <attr name="rightTitle" format="string"/>
    </declare-styleable>
    <declare-styleable name="InputView">
        <attr name="min" format="integer"/>
        <attr name="errorId" format="reference"/>
        <attr name="index" format="integer"/>
    </declare-styleable>
</resources>
